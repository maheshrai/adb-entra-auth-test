# Certificate file path
CERTIFICATE_FILE=/path/to/certificate.pem

# OCI Vault Secrets (OCID of each secret)
APP_ENTRA_TEST_PRIVATE_KEY=ocid1.vaultsecret.oc1...
APP_ENTRA_TEST_PRIVATE_KEY_PWD=ocid1.vaultsecret.oc1...

# Entra ID (Azure AD) Configuration
ENTRA_CLIENT_ID=your-entra-client-id
ENTRA_TENANT_ID=your-entra-tenant-id
ENTRA_SCOPE=api://your-client-id/.default  # optional, defaults to api://{ENTRA_CLIENT_ID}/.default

# Oracle ADB Configuration
ORACLE_TNS_NAME=your-tns-name
TNS_ADMIN=/path/to/tnsnames
WALLET_LOCATION=/path/to/wallet

# OCI Authentication
OCI_TENANCY_OCID=ocid1.tenancy.oc1...
OCI_USER_OCID=ocid1.user.oc1...
OCI_FINGERPRINT=aa:bb:cc:dd:ee:ff:00:11:22:33:44:55:66:77:88:99
OCI_REGION=us-ashburn-1
OCI_KEY_FILE=/path/to/oci-api-key.pem
OCI_KEY_PASSPHRASE=  # optional
